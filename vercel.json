{
    "name": "image-recognition-api",
    "builds": [
        {
            "src": "api/main.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/detect_object",
            "dest": "api/api.py"
        }
    ]
}
